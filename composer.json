{
    "name": "fullscreeninteractive/silverstripe-queuedjob-progressfield",
    "description": "Displays a user friendly form field for the progress of a QueuedJob",
    "type": "silverstripe-vendormodule",
    "keywords": ["silverstripe", "queuedjobs"],
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "Will Rossiter",
        "email": "will@fullscreen.io"
    }],
    "require": {
        "symbiote/silverstripe-queuedjobs": "^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "expose": [
            "client"
        ]
    },
    "autoload": {
        "psr-4": {
            "FullscreenInteractive\\QueuedJobProgressField\\": "src/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
